<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateDatabaseToolWindow">
    <keyboard-shortcut first-keystroke="ctrl alt f3" />
  </action>
  <action id="ActivateGradleToolWindow">
    <keyboard-shortcut first-keystroke="ctrl alt f2" />
  </action>
  <action id="ActivateMavenProjectsToolWindow">
    <keyboard-shortcut first-keystroke="ctrl alt f1" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="CloseAllEditorsButActive">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift ctrl y" second-keystroke="b" />
  </action>
  <action id="editRunConfigurations">
    <keyboard-shortcut first-keystroke="shift alt f11" />
  </action>
</keymap>